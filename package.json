  "devDependencies": {
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.0.38",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-plugin-astro": "^0.26.1",
